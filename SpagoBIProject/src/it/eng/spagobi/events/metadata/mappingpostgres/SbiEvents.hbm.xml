<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="it.eng.spagobi.events.metadata.SbiEvents" table="sbi_events">
        <id name="id" type="integer">
            <column name="id" />
             <generator class="sequence">
            	<param name="sequence">sbi_events_seq</param> 
            </generator>
        </id>
                
        <property name="user" type="string">
            <column name="user_event" length="40" not-null="true" />
        </property>        
    </class>
</hibernate-mapping>
